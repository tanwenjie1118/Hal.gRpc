
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>gRpc</title>
    <!-- Bootstrap CSS -->
<link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
</head>
<body>
    <div class="row" style="margin:20px">
        <div class="col-12"><h1>Website process is running</h1></div>
        <div class="col-12">
            <h1>@Html.Raw(ViewBag.Reply)</h1>
            <button id="pull" class="btn btn-primary">PULL</button>
        </div>
        <div class="col-12" id="content"></div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script>
        "use strict";

        $('#pull').click(e => {
            jQuery.ajax({
                url: "/grpc/getClientDatas",
                type: "GET",
                success: function (res) {
                    var txt = "<h5>" + res + "</h5>";
                    $('#content').append(txt)
                }
            });
        });
    </script>
</body>
</html>
